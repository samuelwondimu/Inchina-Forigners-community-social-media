n  }, [getUserPosts]);\n  return (\n    <Fragment>\n      <div>\n        <hr></hr>\n        <h1 className=\"text-center\">My Posts</h1>\n        {userposts.map((post) => (\n          <div className=\"post p-1 my-1\" style={{ color: '#333' }}>\n            <div>\n              <img className=\"round-img\" src={post.avatar} alt=\"\" />\n              <h4 style={{ color: '#333' }}>{post.name}</h4>\n            </div>\n            <div>\n              {Route() === '/dashboard' ? (\n                <div\n                  dangerouslySetInnerHTML={{\n                    __html: post.text.substring(0, 400).trim() + '...'\n                  }}\n                ></div>\n              ) : (\n                <div\n                  dangerouslySetInnerHTML={{\n                    __html: post.text\n                  }}\n                ></div>\n              )}\n\n              <p className=\"post-date\">Posted on {formatDate(post.date)}</p>\n              {showActions && (\n                <Fragment>\n                  <button\n                    onClick={() => addLike(post._id)}\n                    type=\"button\"\n                    className=\"btn btn-light\"\n                  >\n                    <i className=\"fas fa-thumbs-up\" />{' '}\n                    <span>\n                      {post.likes.length > 0 && (\n                        <span>{post.likes.length}</span>\n                      )}\n                    </span>\n                  </button>\n                  <button\n                    onClick={() => removeLike(post._id)}\n                    type=\"button\"\n                    className=\"btn btn-light\"\n                  >\n                    <i className=\"fas fa-thumbs-down\" />\n                  </button>\n                  <Link to={`/posts/${post._id}`} className=\"btn\">\n                    comments{' '}\n                    {post.comments.length > 0 && (\n                      <span className=\"comment-count\" style={{ color: '#fff' }}>\n                        {post.comments.length}\n                      </span>\n                    )}\n                  </Link>\n                </Fragment>\n              )}\n            </div>\n          </div>\n        ))}\n      </div>\n    </Fragment>\n  );\n};\n\nUserPosts.defaultProps = {\n  showActions: true\n};\n\nUserPosts.propTypes = {\n  auth: PropTypes.object.isRequired,\n  getUserPosts: PropTypes.func.isRequired,\n  post: PropTypes.object.isRequired,\n  profile: PropTypes.object.isRequired,\n  addLike: PropTypes.func.isRequired,\n  removeLike: PropTypes.func.isRequired,\n  deletePost: PropTypes.func.isRequired,\n  showActions: PropTypes.bool\n};\n\nconst mapStateToProps = (state) => ({\n  auth: state.auth,\n  post: state.post,\n  profile: state.profile\n});\n\nexport default connect(mapStateToProps, {\n  addLike,\n  removeLike,\n  deletePost,\n  getUserPosts\n})(UserPosts);\n","/home/samuel/final-project/client/src/components/dashboard/ProfileItem.js",[],"/home/samuel/final-project/client/src/components/profiles/ProfileItem.js",[],"/home/samuel/final-project/client/src/components/profile/ProfileTop.js",[],"/home/samuel/final-project/client/src/components/posts/PostItem.js",[],"/home/samuel/final-project/client/src/components/posts/PostForm.js",[],"/home/samuel/final-project/client/src/components/profile/ProfileAbout.js",[],"/home/samuel/final-project/client/src/actions/post.js",[],"/home/samuel/final-project/client/src/components/post/CommentForm.js",[],"/home/samuel/final-project/client/src/components/post/CommentItem.js",[],"/home/samuel/final-project/client/src/reducers/alert.js",[],"/home/samuel/final-project/client/src/reducers/auth.js",[],"/home/samuel/final-project/client/src/reducers/profile.js",[],"/home/samuel/final-project/client/src/reducers/post.js",[],"/home/samuel/final-project/client/src/utils/formatDate.js",[],"/home/samuel/final-project/client/src/components/dashboard/DashboardActions.js",[],["172","173"],{"ruleId":"174","replacedBy